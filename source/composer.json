{
    "require": {
        "php": "~7.1",
        "ext-gettext": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "woocommerce/woocommerce": "10.0.4"
    },
    "minimum-stability": "stable",
    "extra": {
        "installer-paths": {
            "woocommerce/packages/{$name}/": [
                "woocommerce/action-scheduler",
                "woocommerce/woocommerce-blocks",
                "woocommerce/woocommerce-admin"
            ],
            "{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}
